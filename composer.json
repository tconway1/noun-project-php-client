{
    "name": "rolfisub/noun-project-php-client",
    "description": "A PHP client to interface with The Noun Project",
    "homepage": "https://github.com/rolfisub/noun-project-php-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Radford",
            "email": "mradford@protonmail.com",
            "homepage": "http://mradford.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/tconway1/mattyrad-support"
        }
    ],
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "^7.0",
        "mattyrad/support": "dev-forked-mattyrad-support",
        "guzzlehttp/oauth-subscriber": "^0.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "MattyRad\\NounProject\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MattyRad\\NounProject\\Unit\\Tests\\": "tests/Unit"
        }
    }
}
